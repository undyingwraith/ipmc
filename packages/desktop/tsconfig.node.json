{
	"extends": "@electron-toolkit/tsconfig/tsconfig.node.json",
	"include": [
		"electron.vite.config.*",
		"src/main/*",
		"src/preload/*"
	],
	"compilerOptions": {
		"allowSyntheticDefaultImports": true,
		"composite": true,
		"module": "es2020",
		"target": "es2020",
		"skipLibCheck": true,
		"lib": [
			"ESNext",
		],
		"types": [
			"electron-vite/node",
			"node"
		]
	}
}